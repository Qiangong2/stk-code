{
  "clickable_minimum_required": "6.12.2",
  "builder": "cmake",
  "kill": "STK",
  "arch": "arm64",
  "dependencies_target": [
       "libsdl2-dev",
       "libcurl4-openssl-dev",
       "libenet-dev",
       "libharfbuzz-dev",
       "libfribidi-dev",
       "libgles2-mesa-dev",
       "libjpeg-dev",
       "libogg-dev",
       "libopenal-dev",
       "libpng-dev",
       "libssl-dev",
       "libvorbis-dev",
       "nettle-dev",
       "zlib1g-dev",
       "libbluetooth-dev",
       "libpthread-stubs0-dev",
       "libfreetype6-dev"
  ],
  "dependencies_host": [
       "pkg-config"
  ],
  "image_setup": {
      "run": [
      ]
  },
  "install_lib": [
],
  "build_args": [
      "-DUSE_GLES2=ON",
      "-DNO_IRR_COMPILE_WITH_OPENGL_=ON",
      "-DBUILD_RECORDER=off",
      "-DFREETYPE_INCLUDE_DIRS=/usr/include/${ARCH_TRIPLET}/freetype2",
      "-DFREETYPE_LIBRARIES=/usr/lib/${ARCH_TRIPLET}/freetype.so"
  ]
}
